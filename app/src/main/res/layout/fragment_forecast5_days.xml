<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="forecast5DaysFragmentRecicleViewAdapter"
            type="com.wlmtest.forecasttestjava.view.forecast.tabs.fivedays.Forecast5DaysFragmentRecicleViewAdapter" />

    </data>


    <RelativeLayout
        android:id="@+id/rlMultipleCitiesFound"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="5dp"
        >

        <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:adapter="@{forecast5DaysFragmentRecicleViewAdapter}"
            app:layoutManager="LinearLayoutManager"
            />


    </RelativeLayout>

</layout>